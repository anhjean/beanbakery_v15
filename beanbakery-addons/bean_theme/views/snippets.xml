    <template id="snippet_options" inherit_id="website.snippet_options">
        <xpath expr="." position="inside">
            <!-- Background image, color and shadow -->
            <t t-call="web_editor.snippet_options_background_options">
                <t t-set="selector" t-value="'.tp-editor-bg-both'"/>
                <t t-set="with_colors" t-value="True"/>
                <t t-set="with_images" t-value="True"/>
                <t t-set="with_color_combinations" t-value="True"/>
                <t t-set="with_gradients" t-value="True"/>
            </t>
            <!-- Background image only -->
            <t t-call="web_editor.snippet_options_background_options">
                <t t-set="selector" t-value="'.tp-editor-bg-image'"/>
                <t t-set="with_colors" t-value="False"/>
                <t t-set="with_images" t-value="True"/>
                <t t-set="with_color_combinations" t-value="False"/>
                <t t-set="with_gradients" t-value="False"/>
            </t>
            <!-- Background(without image) and shadow -->
            <t t-call="web_editor.snippet_options_background_options">
                <t t-set="selector" t-value="'.tp-editor-bg-color-n-shadow:not(.card)'"/>
                <t t-set="with_colors" t-value="True"/>
                <t t-set="with_images" t-value="False"/>
                <t t-set="with_color_combinations" t-value="True"/>
                <t t-set="with_gradients" t-value="True"/>
            </t>
            <t t-call="web_editor.snippet_options_background_options">
                <t t-set="selector" t-value="'.tp-editor-bg-color-n-shadow.card'"/>
                <t t-set="with_colors" t-value="True"/>
                <t t-set="with_images" t-value="False"/>
                <t t-set="with_color_combinations" t-value="False"/>
                <t t-set="with_gradients" t-value="True"/>
            </t>
            <div data-js="Box" data-selector=".tp-editor-bg-color-n-shadow">
                <t t-call="website.snippet_options_border_widgets"/>
                <t t-call="website.snippet_options_shadow_widgets"/>
            </div>
            <!-- Duplicate and movable block -->
            <div data-selector=".tp-editor-copy-n-move" data-drop-near=".tp-editor-copy-n-move"/>
        </xpath>
        <!-- Duplicate and movable block -->
        <xpath expr="//div[@data-js='SnippetMove']" position="attributes">
            <attribute name="data-selector" add=".tp-editor-copy-n-move" separator=","/>
        </xpath>
        <!-- Vertical alignment -->
        <xpath expr="//div[@data-option-name='vAlignment']" position="attributes">
            <attribute name="data-selector" add=".tp-editor-snippet-valign" separator=", "/>
        </xpath>
        <!-- Use as a inner content -->
        <xpath expr="//*[@t-set='so_content_addition_selector']" position="inside">, .tp-editor-inner-snippet</xpath>
        <!-- Mega Menu -->
        <xpath expr="//div[@data-js='MegaMenuLayout']/we-select/we-button[last()]" position="after">
            <t t-set="_label">Menus</t>
            <we-button t-att-data-select-label="_label" data-select-template="bean_theme_common.s_mega_menu_bean_1" data-img="/bean_theme/static/src/img/options/s_mega_menu_bean_1.png" t-out="_label"/>
            <t t-set="_label">Menus - Content</t>
            <we-button t-att-data-select-label="_label" data-select-template="bean_theme_common.s_mega_menu_bean_2" data-img="/bean_theme/static/src/img/options/s_mega_menu_bean_2.png" t-out="_label"/>
            <t t-set="_label">Menus - Image - Logos</t>
            <we-button t-att-data-select-label="_label" data-select-template="bean_theme_common.s_mega_menu_bean_3" data-img="/bean_theme/static/src/img/options/s_mega_menu_bean_3.png" t-out="_label"/>
            <t t-set="_label">Content - Menus</t>
            <we-button t-att-data-select-label="_label" data-select-template="bean_theme_common.s_mega_menu_bean_4" data-img="/bean_theme/static/src/img/options/s_mega_menu_bean_4.png" t-out="_label"/>
            <t t-set="_label">Categories - Info</t>
            <we-button t-att-data-select-label="_label" data-select-template="bean_theme_common.s_mega_menu_bean_5" data-img="/bean_theme/static/src/img/options/s_mega_menu_bean_5.png" t-out="_label"/>
            <t t-set="_label">Menus - Images</t>
            <we-button t-att-data-select-label="_label" data-select-template="bean_theme_common.s_mega_menu_bean_6" data-img="/bean_theme/static/src/img/options/s_mega_menu_bean_6.png" t-out="_label"/>
            <t t-set="_label">Categories</t>
            <we-button t-att-data-select-label="_label" data-select-template="bean_theme_common.s_tp_mega_menu_category_snippet" data-img="/bean_theme/static/src/img/options/s_mega_menu_bean_6.png" t-out="_label"/>
