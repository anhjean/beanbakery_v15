                <we-button title="Prime Preheader Style - 4" data-name="tp_preheader_style_4_opt" data-customize-website-views="bean_theme.template_preheader_style_4" data-customize-website-variable="'bean_style_4'" data-img="/bean_theme/static/src/img/options/preheader_4.svg"/>
