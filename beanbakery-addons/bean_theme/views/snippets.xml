            <we-button title="Prime Footer Style - 5" data-name="tp_footer_style_5_opt" data-customize-website-views="bean_theme.template_footer_style_5" data-customize-website-variable="'bean_style_5'" data-img="/bean_theme/static/src/img/options/footer_5.svg"/>
